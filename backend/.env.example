# PromptLens Backend Configuration
# Copy this file to .env and fill in your API keys

# OpenAI API Key (required for text generation)
OPENAI_API_KEY=your_openai_api_key_here

# Replicate API Token (for Stable Diffusion image generation)
REPLICATE_API_TOKEN=your_replicate_api_token_here

# Application Settings
DEBUG=true
CORS_ORIGINS=http://localhost:5173,http://localhost:3000,http://127.0.0.1:5173

# Model Configuration
TEXT_MODEL=gpt-4o-mini
IMAGE_MODEL=stability-ai/stable-diffusion:27b93a2413e7f36cd83da926f3656280b2931564ff050bf9575f1fdf9bcd7478
